    <owl:Class rdf:about="http://purl.org/bmo/swot#SwotAnalysis">
        <rdfs:label xml:lang="en">SWOT analysis</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/bmo/swot/hasStrategicFit"/>
                <owl:allValuesFrom>
